    "@vitejs/plugin-vue": "^5.0.5",
    "bumpp": "^9.4.1",
    "commitizen": "^4.3.0",
    "eslint": "9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "esno": "^4.7.0",
    "husky": "^9.0.11",
