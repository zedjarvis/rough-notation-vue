{
  "name": "rough-notation-vue",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "description": "A vue3 wrapper for rough notation with ts support",
  "author": {
    "name": "Cedrouseroll Omondi",
    "email": "omondicedo@gmail.com",
    "url": "https://zedjarvis.github.io"
  },
  "license": "MIT",
  "readme": "./README.md",
  "homepage": "https://rough-notation-vue.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zedjarvis/rough-notation-vue"
  },
  "bugs": "https://github.com/zedjarvis/rough-notation-vue/issues",
  "keywords": [
    "rough-notation-vue",
