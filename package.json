    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vue": "^3.4.31",
    "vue-tsc": "^2.0.21"
